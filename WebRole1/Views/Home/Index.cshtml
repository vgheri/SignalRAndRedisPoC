    <script type="text/javascript" language="javascript">
        $(document).ready(function () {
            // hide alert message
            $(".alert-success").hide();

            // Reference the auto-generated proxy for the hub.  
            var auction = $.connection.auctionHub;

            // Create a function that the hub can call back to display messages.
            auction.client.notifyBidPlaced = function (amount) {
                // Add the message to the page. 
                var self = $(".alert-success");                
                var message = 'A new bid of ' + amount + ' has been placed on this auction. '+ "\n";
                autoClosingAlert(self, 5000, message);
            };

            auction.client.followedAuction = function(customerId, auctionNr) {
                var self = $(".alert-success");
                var message = 'A new customer ' + customerId + ' follow the auction ' + auctionNr + "\n";
                autoClosingAlert(self, 5000, message);
            };

            function autoClosingAlert(selector, delay, message) {
                
                selector.append(message);
                selector.show();                
                window.setTimeout(function() {                     
                    selector.hide(); 
                    selector.empty();
                }, delay);
            }
            $.connection.hub.error(function (error) {
                console.log('SignalR error: ' + error)
            });
            // Start the connection.
            $.connection.hub.start()
                .done(function() {
                    $('#it_btnFollow').click(function() {
                        auction.server.followAuction();
                    });
                    $('#it_btnBid').click(function() {
                        var amount = $("#it_maxBid").val();
                        auction.client.notifyBidPlaced(amount);
                        auction.server.bidAuction(amount);
                    });
                })
                .fail(function(err) {
                    alert(err);
                });
        });
